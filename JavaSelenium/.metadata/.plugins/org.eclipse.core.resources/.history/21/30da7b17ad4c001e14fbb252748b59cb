package InterviewCoding;

public class CywareLabs {

	public static void main(String[] args) {


		
		//input arr={1,2,3,4,5}
		//Output arr={5,4,3,2,1}-> Reverse array with out creating new array
		
		 int[] arr={1,2,3,4,5};

	        int j=arr.length-1;

	        for(int i=0;i<arr.length/2;i++)
	        {
	            int temp=arr[i];
	            arr[i]=arr[j];
	            arr[j]=temp;
	            j--;

	        }
	        System.out.println(arr);
		
		

	}

}
